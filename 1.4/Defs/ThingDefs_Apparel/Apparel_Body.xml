		<techLevel>Medieval</techLevel>
		<recipeMaker>
			<researchPrerequisites>
				<li>ComplexClothing</li>
				<li>Mashed_Lynian_FelyneEquipment</li>
			</researchPrerequisites>
			<skillRequirements>
				<Crafting>3</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
				<li>Mashed_Lynian_CraftingBench</li>
			</recipeUsers>
		</recipeMaker>
