<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<RulePackDef>
		<defName>Mashed_Lynian_NamerPerson_Utility</defName>
		<rulePack>
			<rulesFiles>
				<li>MogaFarmName->Mashed_Lynians/Lynians_CatLikeNames_Farm</li>
				<li>FlowerNames->Mashed_Lynians/Lynians_UrukiNames_Flowers</li>
				<li>SucculentNames->Mashed_Lynians/Lynians_UrukiNames_Succulents</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	<!-- ==========[Old-World Cat-like pawn namers]==========-->
	<RulePackDef>
		<defName>Mashed_Lynian_NamerPerson_CatLike_OldWorldMale</defName>
		<include>
			<li>Mashed_Lynian_NamerPerson_Utility</li>
			<li>NamerAnimalUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name(p=1)->[MogaFarmName]</li>
				<li>name(p=1)->[NameAnimalUnisex]</li>
				<li>name(p=0.25)->[NameAnimalMale]</li>
				<li>name(p=0.25)->[Vegetable]</li>
				<li>name(p=0.15)->[Animal]</li>
				<li>name(p=0.15)->[Color]</li>
				<li>name(p=0.15)->[ColorBadass]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
