				<li>name(p=0.25)->[Vegetable]</li>
				<li>name(p=0.15)->[Animal]</li>
				<li>name(p=0.15)->[Color]</li>
				<li>name(p=0.15)->[ColorBadass]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
