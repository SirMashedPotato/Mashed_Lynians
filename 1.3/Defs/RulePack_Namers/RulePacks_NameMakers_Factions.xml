	<RulePackDef>
		<defName>Mashed_Lynian_NamerSettlementCatLike</defName>
		<include>
			<li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name->[Color] [TerrainFeature]</li>
