<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<TraderKindDef>
		<defName>Mashed_Lynian_OrbitalTrader</defName>
		<label>Lynian guild trader</label>
		<orbital>true</orbital>
		<requestable>false</requestable>
		<commonalityMultFromPopulationIntent>
			<points>
				<li>(0.2, 0)</li>
				<li>(1,   1)</li>
				<li>(2,   3)</li>
			</points>
		</commonalityMultFromPopulationIntent>
		<stockGenerators>
			<!-- Resources -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>1000~2000</countRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>FoodRaw</categoryDef>
				<excludedCategories>
					<li>EggsFertilized</li>
				</excludedCategories>
				<thingDefCountRange>2~4</thingDefCountRange>
				<totalPriceRange>300~500</totalPriceRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>FoodMeals</categoryDef>
				<thingDefCountRange>2~5</thingDefCountRange>
				<countRange>10~20</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Lynian_LynianDyeKit</thingDef>
				<countRange>3~5</countRange>
			</li>
			<!-- Drugs -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Lynian_FelvinePowder</thingDef>
				<countRange>75~150</countRange>
			</li>
			<!-- Apparel -->
			<li Class="StockGenerator_MarketValue">
				<tradeTag>Mashed_Lynian_LynianWorker</tradeTag>
				<countRange>3~9</countRange>
			</li>
			<li Class="StockGenerator_MarketValue">
				<tradeTag>Mashed_Lynian_LynianAcornKnight</tradeTag>
				<countRange>1~5</countRange>
			</li>
			<!-- Special -->
			<li Class="StockGenerator_MarketValue">
				<tradeTag>Mashed_Lynian_Eurekacorn</tradeTag>
				<countRange>1~3</countRange>
			</li>
			<li Class="StockGenerator_MarketValue">
				<tradeTag>Mashed_Lynian_ShakalakaDanceBook</tradeTag>
				<countRange>-2~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Lynian_LynianNotes</thingDef>
				<countRange>1</countRange>
			</li>
			<!-- Genepacks -->
			<li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_SingleDef">
				<thingDef>Genepack</thingDef>
				<countRange>1~2</countRange>
			</li>
			<!-- Recruits -->
			<li Class="Mashed_Lynians.StockGenerator_Palicoes">
				<respectPopulationIntent>False</respectPopulationIntent>
				<pawnKindDefList>
					<li>Mashed_Lynian_BoaboaRecruit</li>
					<li>Mashed_Lynian_FelyneRecruit</li>
					<li>Mashed_Lynian_GajalakaRecruit</li>
					<li>Mashed_Lynian_GrimalkyneRecruit</li>
					<li>Mashed_Lynian_MelynxRecruit</li>
					<li>Mashed_Lynian_ShakalakaRecruit</li>
					<li>Mashed_Lynian_UrukiRecruit</li>
				</pawnKindDefList>
				<countRange>3~6</countRange>
			</li>
			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple"/>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Art</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ImplantEmpireCommon</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>ImplantEmpireRoyal</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>PsylinkNeuroformer</tag>
			</li>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>MusicalInstrument</tag>
			</li>
		</stockGenerators>
	</TraderKindDef>
</Defs>
